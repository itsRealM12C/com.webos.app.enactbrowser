{
  "manifest_version": 2,
  "name": "Enact-based Browser Sample",
  "version": "0.1",
  "version_name": "v0.1 [build_number_placeholder]",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webview",
    "storage",
    "https://*/*",
    "http://*/*",
    "fullscreen",
    "audioCapture",
    "videoCapture"
  ]
}
