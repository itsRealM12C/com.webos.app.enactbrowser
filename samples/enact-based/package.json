{
  "name": "com.webos.app.browser",
  "version": "1.0.0",
  "description": "A enact based web browser",
  "author": "",
  "main": "src/index.js",
  "scripts": {
    "serve": "enact serve",
    "build": "./scripts/build.sh",
    "build-p": "./scripts/build.sh -p",
    "pack": "enact pack",
    "pack-p": "enact pack -p",
    "watch": "enact pack --watch",
    "clean": "enact clean",
    "clean-all": "./scripts/clean-all.sh",
    "lint": "enact lint .",
    "license": "enact license",
    "test": "enact test start --single-run --browsers PhantomJS",
    "test-json": "enact test start --single-run --browsers PhantomJS --reporters json",
    "test-watch": "enact test start --browsers PhantomJS"
  },
  "license": "LicenseRef-EnactBrowser-Evaluation",
  "private": true,
  "repository": "",
  "enact": {
    "isomorphic": true,
    "ri": {
      "baseSize": 24
    },
    "template": "template-webos.ejs"
  },
  "eslintConfig": {
    "extends": "enact"
  },
  "dependencies": {
    "js-browser-lib": "file:../../lib",
    "@enact/core": "^3.0.0",
    "@enact/i18n": "^3.0.0",
    "@enact/moonstone": "^3.0.0",
    "@enact/spotlight": "^3.0.0",
    "@enact/ui": "^3.0.0",
    "@enact/webos": "^3.0.0",
    "ilib": "^14.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "redux": "^3.7.2",
    "react-redux": "^5.0.7"
  },
  "devDependencies": {
    "minimist": "^1.2.0"
  }
}
